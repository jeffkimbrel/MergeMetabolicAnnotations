{
  "name" : "Merge Metabolic Annotations",
  "ver" : "0.0.1",
  "authors" : ["jakimbrel","chenry","patrikd"],
  "contact" : "http://kbase.us/contact-us/",
  "visble" : true,
  "categories" : ["active","annotation"],
  "widgets" : {
    "input" : null,
    "output" : ""
  },
  "parameter-groups": [
       {
           "id": "annotations_to_merge",
           "parameters": [
               "annotation_source",
               "annotation_weight"
           ],
           "optional": false,
           "advanced": false,
           "allow_multiple": true,
           "with_border": true
       }
   ],
  "parameters" : [
     {
       "id" : "genome",
       "optional" : false,
       "advanced" : false,
       "allow_multiple" : false,
       "default_values" : [ "" ],
       "field_type" : "text",
       "text_options" : {
         "valid_ws_types" : [ "KBaseGenomes.Genome" ]
       }
     },
     {
        "id" : "annotation_source",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "textsubdata",
        "textsubdata_options" : {
           "allow_custom" : false,
           "show_src_obj" : false,
           "subdata_selection" : {
              "path_to_subdata" : [
                 "ontology_events"
              ],
              "parameter_id" : "genome",
              "subdata_included" : [
                 "ontology_events/[*]/id",
                 "ontology_events/[*]/timestamp",
                 "ontology_events/[*]/description",
                 "ontology_events/[*]/method"
              ],
              "selection_description": ["id", "timestamp", "description", "method"],
              "description_template" : "<strong>{{id}}</strong> ontology terms from <strong>{{method}}</strong> added <strong>{{timestamp}}</strong>",
              "selection_id" : "description"
           },
           "multiselection" : false
        },
        "text_options" : {
           "valid_ws_types" : []
        }
     },
     {
       "id" : "annotation_weight",
       "optional" : false,
       "advanced" : false,
       "allow_multiple" : false,
       "default_values" : [ 1 ],
       "field_type" : "text",
       "text_options" : {"validate_as" : "float"}
     },
     {
       "id" : "annotation_threshold",
       "optional" : false,
       "advanced" : false,
       "allow_multiple" : false,
       "default_values" : [ 1 ],
       "field_type" : "text",
       "text_options" : {"validate_as" : "float"}
     },
     {
        "field_type" : "checkbox",
        "allow_multiple" : false,
        "optional" : false,
        "id" : "keep_best_annotation_only",
        "advanced" : false,
        "default_values" : [
           "best"
        ],
        "checkbox_options" : {
           "unchecked_value" : "all",
           "checked_value" : "best"
        },
        "text_options" : {
           "valid_ws_types" : []
        }
     },
     {
       "id" : "description",
       "optional" : false,
       "advanced" : false,
       "allow_multiple" : false,
       "default_values" : [ "Merged annotations" ],
       "field_type" : "text",
       "text_options" : {}
     },
     {
       "id" : "output_name",
       "optional" : false,
       "advanced" : false,
       "allow_multiple" : false,
       "default_values" : [ "" ],
       "field_type" : "text",
       "text_options" : {
         "valid_ws_types" : [ "KBaseGenomes.Genome" ],
         "is_output_name" : true
       }
     }
  ],
  "behavior" : {
    "service-mapping": {
       "url": "",
       "name": "MergeMetabolicAnnotations",
       "method": "merge_metabolic_annotations",
       "input_mapping": [
           {
               "narrative_system_variable": "workspace",
               "target_property": "workspace_name"
           },
           {
               "target_property" : "genome",
               "input_parameter" : "genome",
               "target_type_transform": "resolved-ref"
           },
           {
               "target_property" : "annotations_to_merge",
               "input_parameter" : "annotations_to_merge"
           },
           {
               "target_property" : "annotation_threshold",
               "input_parameter" : "annotation_threshold"
           },
           {
               "target_property" : "keep_best_annotation_only",
               "input_parameter" : "keep_best_annotation_only"
           },
           {
               "target_property" : "description",
               "input_parameter" : "description"
           },
           {
               "target_property" : "output_name",
               "input_parameter" : "output_name"
           }
       ],
      "output_mapping" : [
     {
             "narrative_system_variable": "workspace",
             "target_property": "wsName"
           },
           {
             "service_method_output_path": [0,"obj_ref"],
             "target_property": "obj_ref",
             "target_type_transform": "resolved-ref"
           },
           {
               "service_method_output_path": [0, "report_name"],
               "target_property": "report_name"
           },
           {
               "service_method_output_path": [0, "report_ref"],
               "target_property": "report_ref"
           },
           {
               "constant_value": "16",
               "target_property": "report_window_line_height"
           }
      ]
    }
  },
  "job_id_output_field": "docker"
}
